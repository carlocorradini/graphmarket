{
  "name": "graphmarket",
  "version": "1.0.0",
  "description": "Online e-commerce API in GraphQL",
  "engines": {
    "node": "^10.3",
    "npm": "^6.9.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/carlocorradini/graphmarket",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlocorradini/graphmarket"
  },
  "bugs": {
    "url": "https://github.com/carlocorradini/graphmarket/issues",
    "email": "carlo.corradini@studenti.unitn.it"
  },
  "main": "./build/app.js",
  "private": true,
  "author": {
    "name": "Carlo Corradini",
    "email": "carlo.corradini@studenti.unitn.it",
    "url": "https://github.com/carlocorradini"
  },
  "contributors": [
    {
      "name": "Andrea Stedile",
      "email": "andrea.stedile@studenti.unitn.it",
      "url": "https://github.com/andreastedile"
    }
  ],
  "scripts": {
    "clean": "npx rimraf build docs log coverage",
    "update": "npx ncu -u && npm install",
    "lint": "npx eslint --ext .ts \"src/**/*.ts\" \"scripts/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --config jest.config.ts --no-watchman --silent",
    "test:verbose": "npm run test -- --verbose --silent false",
    "coverage": "npm run test -- --coverage",
    "predocs": "npm run clean",
    "docs": "npx typedoc",
    "prebuild": "npm run clean",
    "build": "npx ttsc --p ./tsconfig.json",
    "postbuild": "npx ts-node scripts/build.ts",
    "start": "node build/app.js",
    "start:dev": "npx ts-node-dev --prefer-ts true --no-notify --rs --clear --exit-child -r tsconfig-paths/register --watch src --respawn --transpile-only src/app.ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.5.0",
    "helmet": "^4.2.0",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "typeorm-typedi-extensions": "^0.2.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "^5.1.5",
    "@types/ioredis": "^4.17.8",
    "@types/jest": "^26.0.16",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "npm-check-updates": "^10.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "typescript-transform-paths": "^2.1.0"
  }
}
