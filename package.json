    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "twilio": "^3.54.1",
    "type-graphql": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
