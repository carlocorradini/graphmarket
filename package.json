{
  "name": "graphmarket",
  "version": "1.0.0",
  "description": "Online e-commerce API in GraphQL",
  "engines": {
    "node": "^10.3",
    "npm": "^6.9.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/carlocorradini/graphmarket",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlocorradini/graphmarket"
  },
  "bugs": {
    "url": "https://github.com/carlocorradini/graphmarket/issues",
    "email": "carlo.corradini@studenti.unitn.it"
  },
  "main": "./build/index.js",
  "private": true,
  "author": {
    "name": "Carlo Corradini",
    "email": "carlo.corradini@studenti.unitn.it",
    "url": "https://github.com/carlocorradini"
  },
  "contributors": [
    {
      "name": "Andrea Stedile",
      "email": "andrea.stedile@studenti.unitn.it",
      "url": "https://github.com/andreastedile"
    }
  ],
  "scripts": {
    "clean": "npx rimraf build docs log coverage",
    "update": "npx ncu -u && npm install",
    "lint": "npx eslint --ext .ts \"src/**/*.ts\" \"scripts/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --config jest.config.ts --no-watchman --silent",
    "test:debug": "npm run test -- --verbose --silent false",
    "coverage": "npm run test -- --coverage",
    "predocs": "npm run clean",
    "docs": "npx typedoc",
    "prebuild": "npm run clean",
    "build": "npx ttsc --p ./tsconfig.json",
    "postbuild": "npx ts-node scripts/build.ts",
    "start": "node build/app.js",
    "start:dev": "npx cross-env NODE_ENV=development ts-node-dev --prefer-ts true --no-notify --rs --clear --exit-child -r tsconfig-paths/register --watch src --respawn --transpile-only src/app.ts"
  },
  "dependencies": {
    "@apollo/gateway": "^0.21.4",
    "apollo-server": "^2.19.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.7.0",
    "lodash": "^4.17.20",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "typescript-transform-paths": "^2.1.0"
  }
}
