  "scripts": {
    "clean": "npx rimraf build docs log coverage",
    "lint": "npx eslint --ext .ts \"src/**/*.ts\" \"scripts/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --config jest.config.ts --no-watchman --silent",
    "test:debug": "npm run test -- --verbose --silent false",
    "coverage": "npm run test -- --coverage",
    "predocs": "npm run clean",
    "docs": "npx typedoc",
    "prebuild": "npm run clean",
    "build": "npx ttsc --project tsconfig.json",
    "postbuild": "npx ts-node scripts/build.ts",
    "start": "node build/index.js",
    "start:dev": "npx cross-env NODE_ENV=development ts-node-dev --prefer-ts true --no-notify --rs --clear --exit-child -r tsconfig-paths/register --watch src --respawn --transpile-only src/bootstrap.ts"
  },
